

@foreach (var category in ViewBag.eventViewModels)
{
    <a asp-action="Details" asp-controller="Category" asp-route-id="@category.CategoryId">    <h1>@category.Name</h1> </a>

    <h1>@category.NumOfTickets</h1>
    <a asp-action="Delete" asp-controller="Category" asp-route-id="@category.CategoryId">    <h1>Delete @category.Name </h1> </a>
    <a asp-action="Update" asp-controller="Category" asp-route-id="@category.CategoryId">    <h1>Edit @category.Name </h1> </a>


}

<a class="btn btn-primary" asp-action="Create" asp-controller="Category"> Create Category</a>